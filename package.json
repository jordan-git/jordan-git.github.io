    "node-sass": "^4.14.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^5.2.0"
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "devDependencies": {
    "@types/styled-components": "^5.1.3",
    "gh-pages": "^3.1.0",
    "prettier": "2.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jordan-git/portfolio-website"
  },
  "bugs": {
